{
    "root": true,
    "overrides": [
        {
            "env": {
                "es6": true,
                "node": true
            },
            "files": [
                "library/**/*.js",
                "module/**/*.js",
                "env.js",
                "app.js"
            ],
            "extends": [
                "eslint:recommended",
                "plugin:import/recommended"
            ],
            "plugins": [
                "prefer-arrow"
            ]
        }
    ],
    "ignorePatterns": [
        "public/**/*.js"
    ]
}